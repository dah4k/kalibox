unalias -a

alias ..="cd .."
alias -- +="pushd ."
alias -- -="popd"
alias date="LC_TIME=en_GB.UTF-8 date"

alias fd="fdfind"
alias ip="ip --color=auto"
alias la="ls -Altr"
alias ls="ls -F --color=auto"
alias rg="rg --sort-files"
alias v="vim -R"
